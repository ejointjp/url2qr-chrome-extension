{
  "name": "URL2QR",
  "description": "URL2QRのChrome拡張機能です。1クリックでWebページのURLのQRコードを作成できます。",
  "version": "0.0.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "今表示しているページのQRコードを作成します"
  },
  "background": {
    "scripts": [ "script.js" ]
  },
  "homepage_url": "https://url2qr.e-joint.jp"
}
